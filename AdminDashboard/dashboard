    var UIIdleTimeout = function () {

    return {

        //main function to initiate the module
        init: function () {

            // cache a reference to the countdown element so we don't have to query the DOM for it on each ping.
            var $countdown;

            $('body').append('<div style="color:#000" class="modal fade" id="idle-timeout-dialog" data-backdrop="static"><div class="modal-dialog modal-small"><div class="modal-content"><div class="modal-header"><h4 class="modal-title">Session Anda akan berakhir.</h4></div><div class="modal-body"><p><i class="fa fa-warning"></i> Session Anda akan dikunci pada <span id="idle-timeout-counter"></span> detik.</p><p>Apakah Anda ingin melanjutkan SSO?</p></div><div class="modal-footer"><button id="idle-timeout-dialog-logout" type="button" class="btn btn-danger">Tidak, saya ingin keluar</button><button id="idle-timeout-dialog-keepalive" type="button" class="btn btn-primary" data-dismiss="modal">Iya, biarkan saya tetap aktif.</button></div></div></div></div>');
                
            // start the idle timer plugin
            $.idleTimeout('#idle-timeout-dialog', '.modal-content button:last', {
                idleAfter: 3600, // 3600 seconds or 60 mins value 60
                timeout: 3600, //60 seconds to timeout value 60000
                pollingInterval: 3600, // 600 seconds value 600
                keepAliveURL: "https://sso.unesa.ac.id/user/idletimeout-keepalive",
                serverResponseEquals: 'OK',
                onTimeout: function(){
                    //TIMEOUT
                    window.location.replace("https://sso.unesa.ac.id/dashboard/logout");
                    
                },
                onIdle: function(){ 
                   
                    var
                        closeInSeconds = 60,
                        displayText = "Session Anda akan berakhir pada #60 detik. Apakah Anda ingin melanjutkan SSO?",
                        timer;

                    swal({
                        title: "Session Anda akan berakhir.",   
                        text: displayText.replace(/#60/, closeInSeconds),   
                        timer: closeInSeconds * 1000,   
                        //showConfirmButton: false 
                        type: "warning",
                        showCancelButton: true,
                        confirmButtonColor: '#0CF',
                        confirmButtonText: 'Iya, biarkan saya tetap aktif.',
                        cancelButtonText: "Tidak, saya ingin keluar.",
                        closeOnConfirm: false,
                        closeOnCancel: true
                        },
                    function(isConfirm){
                      if (isConfirm) {
                        //swal("Deleted!", "Your imaginary file has been deleted.", "success");
                        window.location.replace("https://sso.unesa.ac.id/user");
                      } else {
                        swal("Logout", "Anda akan logout dari SSO sesaat lagi", "error");
                        window.location.replace("https://sso.unesa.ac.id/dashboard/logout");
                      }
                    });

                    

                    timer = setInterval(function() {

                        closeInSeconds--;

                        if (closeInSeconds < 0) {

                            clearInterval(timer);
                        }

                        $('.sweet-alert > p').text(displayText.replace(/#60/, closeInSeconds));

                    }, 1000);
                    /*$('#idle-timeout-dialog').modal('show');
                    $countdown = $('#idle-timeout-counter');
                    
                    $('#idle-timeout-dialog-keepalive').on('click', function () { 
                        $('#idle-timeout-dialog').modal('hide');
                        window.location.reload();
                    });

