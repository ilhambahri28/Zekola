		<script type="text/javascript">

		function logout(){
       	  
       	  Swal.fire({
		        title: "Apa Anda yakin?",
		        text: "ingin keluar dari aplikasi SSO UNESA?",
		        icon: "warning",
		        showCancelButton: true,
		        confirmButtonColor: '#0CF',
		        confirmButtonText: "Ya",
		        cancelButtonText: "Tidak",
		        reverseButtons: true
		    }).then(function(result) {
		        if (result.value) {
		           window.location.replace("https://sso.unesa.ac.id/dashboard/logout");
		        } else if (result.dismiss === "cancel") {
		        }
		    });
 
    	}

		function pilihkategori(kategori,nama)
		{
			document.getElementById("sim-dd402b92-dafd-4396-a900-13237feb6fd6").style = "display:none";
						document.getElementById("sim-96379f60-7553-4a61-b90c-f1a700b37476").style = "display:none";
			document.getElementById("sim-f69ec57f-c1b5-4d03-81e8-e4e474669ae2").style = "display:none";
			document.getElementById("sim-673bb7d7-6098-40ff-8b70-44e2ae2e4fea").style = "display:none";
			document.getElementById("sim-c9a63a4d-98e3-4b6a-bb90-c60dd13c51e5").style = "display:none";

			document.getElementById("namakategori").innerHTML = nama;
			document.getElementById("sim-"+kategori).style = "display:block";
		}

</script>
