}();
    
    
    $(document).ready(
        function()
        {   
        	                  		
            function loadberitaunesa(){
	          	document.getElementById("berita-unesa").innerHTML = "<br><br><br><br><br><center>loading...</center><br><br><br><br><br>";
	                $.ajax({ url: "https://sso.unesa.ac.id/api/berita-unesa",
	                         type : "GET",
	                         dataType: "json",
	                         success: function(result){ 
	                         	document.getElementById("berita-unesa").innerHTML = "";
	                         	//result.length
	                            for (var i = 0; i<5; i++) {
	                              document.getElementById("berita-unesa").innerHTML += "<div class='d-flex flex-stack mb-5'> <div class='symbol symbol-50px me-3'><div class='symbol-label bg-light'><img src='"+result[i]['image']+"' style='width:60px;height:55px;' alt='' /></div></div><div><a target='_blank' style='text-decoration:none;color:#000; cursor:pointer' href='"+ result[i]['link'] +"' class='fs-6 text-gray-800 text-hover-primary fw-bolder text-justify'>" + result[i]['title'] + "</a> </div> <div class='badge badge-light fw-bold py-4 px-3'>"+result[i]['pubDate']+" <br><br> "+result[i]['views']+" dilihat </div></div> ";
	                            }
	                }});
            }
        	
        	function loadberitaunesaDetail(){
                document.getElementById("judulberitaunesa").innerHTML = "loading...";
	                $.ajax({ url: "https://sso.unesa.ac.id/api/berita-unesa/detail",
	                         type : "GET",
	                         dataType: "json",
	                         success: function(result){

	                              document.getElementById("judulberitaunesa").innerHTML = "<a target='_blank' style='color:#000;text-shadow:0px 0px 2px; cursor:pointer' href='"+ result['link'] +"'>"+result['title']+"</a>";
	                              document.getElementById("beritaunesa").innerHTML = "<br><center><img style='width:250px; height:200px;float:none;' src='" + result['image'] + "'></center>";
	                              document.getElementById("cuplikanberitaunesa").innerHTML = result['cuplikan'] + "<br><a target='_blank' style='color:#000; cursor:pointer' href='"+ result['link'] +"'>Selengkapnya...</a>";
	                              document.getElementById("tanggalberitaunesa").innerHTML = result['pubDate'];
	                }});
            }

                        	  //$('#kt_modal_create_app').modal('show');
          	  //$('#kt_form_pks').modal('show');
          	  function loadKatiya()
          	  {
          	  	$('#kt_katya').modal('show');
          	  }
          	  window.onload = function loadKatiya() {
				   setTimeout(function () {
				   loadKatiya();
				  },30000)};
          	  	          loadberitaunesa();
	          loadberitaunesaDetail();
	          
	      	  	      	  $('#kt_form_udim').modal('show');
	          $.simpleTicker($("#ticker-roll"),{'effectType':'roll'});
	          //UIIdleTimeout.init();
           }
           );
           </script>
